<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffc7c7c7" android:endColor="#ffdcdcdc" android:angle="270.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffc2f90a" android:endColor="#ff6cc00b" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff92da0b" />
            </shape>
        </clip>
    </item>
</layer-list>
